#!/bin/sh
# TC commands need the 'tc-' prefix in bash.
# (tc-alias.sh is expected to be in the PATH)
. tc-alias.sh

param load
param set CBRK_SUPPLY_CHK 894281

dataman start

battery_simulator start
simulator_mavlink start
tone_alarm start
pwm_out_sim start

ver all

mavlink start -x -u 14556 -r 2000000 -p
mavlink boot_complete

@cmd_name@ start
sleep 1
@cmd_name@ start
sleep 1
@cmd_name@ stop

dataman status

shutdown
